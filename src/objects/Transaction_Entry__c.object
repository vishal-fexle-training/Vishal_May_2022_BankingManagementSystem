    <validationRules>
        <fullName>Check_Contact_Status_for_Tansaction</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISPICKVAL(Contact__r.Status__c, &quot;Inactive&quot;), 
    ISPICKVAL(Contact__r.Status__c,&quot;&quot;) )</errorConditionFormula>
        <errorMessage>Transaction cannot be performed because Contact is Inactive!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Entry_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Transaction_Date__c !=  TODAY()</errorConditionFormula>
        <errorMessage>Invalid Date selected! Select Today&apos;s Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Minimum_Balance</fullName>
        <active>true</active>
        <description>User shouldn&apos;t be able to debit amount if available balance is less than 3000</description>
        <errorConditionFormula>AND( ISPICKVAL(Type__c, &quot;Debit&quot;),
    (Contact__r.Available_Balance__c -  Amount__c ) &lt; 3000)</errorConditionFormula>
        <errorMessage>Cannot debit amount because Minimum Balance should be 3000.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Per_Transaction_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Type__c , &quot;Debit&quot;) ,
    Amount__c  &gt; Contact__r.Transaction_Limit__r.Maximum_Debit_Allowed_per_Transaction__c)</errorConditionFormula>
        <errorMessage>Per Transaction limit exceeds !</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
